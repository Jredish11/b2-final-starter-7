<h1>Merchant Coupons Index</h1>

<div> 
  <p><%= link_to "Create New Coupon", new_merchant_coupon_path(@merchant) %></p>
</div>

  <% @coupons.each do |coupon| %>
  <p>Name: <%= link_to "#{coupon.coupon_name}", merchant_coupon_path(@merchant, coupon) %></p>
  <p>Amount off: <%= coupon.discount_amount %></p> 
<% end %>
